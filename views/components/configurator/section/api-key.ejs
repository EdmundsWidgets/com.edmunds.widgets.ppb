<div class="control-group api-key">
	<label>
		API key (<a href="http://edmunds.mashery.com/member/my-account" target="_blank">get yours</a>)
		<sup class="label label-important label-required">required</sup>
	</label>
	<div class="input-group-control" id="vehicle-api-key-control">
		<div class="input-group input-group-sm" data-section="apply">
			<input type="text" class="form-control" id="vehicle-api-key-input" name="vehicleapikey">
			<span class="input-group-btn">
				<button class="btn btn-default" type="button" id="vehicle-api-key-apply">Apply</button>
			</span>
		</div>
		<div class="input-group input-group-sm hidden" data-section="change">
			<span class="form-control form-control-text"></span>
			<span class="input-group-btn">
				<button class="btn btn-link" type="button">Change</button>
			</span>
		</div>
	</div>
</div>
